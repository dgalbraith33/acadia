interface Denali {
  method Read(ReadRequest) -> (ReadResponse); 
  method ReadMany(ReadManyRequest) -> (ReadResponse);
}

message ReadRequest {
  u64 device_id;
  u64 lba;
  u64 size;
}

message ReadManyRequest {
  u64 device_id;
  repeated u64 lba;
}

message ReadResponse {
  u64 device_id;
  u64 size;
  capability memory;
}

